<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
                 version="1.0">

    <access>FIELD</access>

    <entity class="fr.cla.ddd.metamodel.exampleapp.domain.equatability.sameruntimetype.SrtConference">
        <attributes>
            <one-to-many name="talks" fetch="EAGER">
                <join-column name="CONFERENCE_ID"></join-column>
                <cascade><cascade-all/></cascade>
            </one-to-many>
            <embedded name="cost"></embedded>
        </attributes>
    </entity>

    <entity class="fr.cla.ddd.metamodel.exampleapp.domain.equatability.sameruntimetype.SrtTalk">
        <attributes>
            <embedded name="cost"></embedded>
        </attributes>
    </entity>

</entity-mappings>